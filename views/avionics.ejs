        list-style: none;
        margin-left: -10%;
      }

      .sidebar ul li {
        margin-bottom: 15px;
        margin-left: -5%;
      }

      .sidebar ul li a {
        color: #ecf0f1;
        text-decoration: none;
        display: block;
        padding: 10px;
        border-radius: 4px;
        transition: background 0.3s;
      }

      .sidebar ul li a:hover {
