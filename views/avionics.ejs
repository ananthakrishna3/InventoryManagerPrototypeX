    <style>
      * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
        font-family: Arial, sans-serif;
      }

      body {
        display: flex;
        height: 100vh;
        /* width:max-content; */
        background-color: #f4f4f4;
      }

      .container {
        display: flex;
        width: 100%;
      }

      .sidebar {
        width: 250px;
        background-color: #0c479f;
        padding: 20px;
        color: #fff;
        margin-left: -9.5%;
      }

      

